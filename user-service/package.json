{
  "name": "skillbridge-user-service",
  "version": "1.0.0",
  "description": "User management service for SkillBridge platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "setup:tables": "node scripts/create-dynamodb-tables.js",
    "deploy:cloudformation": "aws cloudformation deploy --template-file scripts/cloudformation-dynamodb.yaml --stack-name skillbridge-user-college --parameter-overrides Environment=college"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "joi": "^17.11.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "uuid": "^9.0.1",
    "axios": "^1.6.2",
    "@aws-sdk/client-dynamodb": "^3.637.0",
    "@aws-sdk/lib-dynamodb": "^3.637.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "user-management",
    "microservice",
    "node",
    "express",
    "typescript"
  ],
  "author": "SkillBridge Team",
  "license": "MIT"
}